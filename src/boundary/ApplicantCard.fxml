<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="TOP_CENTER" spacing="40.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.ApplicantCard">
   <children>
      <Label fx:id="devName" prefHeight="32.0" text="Name" />
      <Button mnemonicParsing="false" text="View Profile" />
      <Button fx:id="sendOffer" layoutX="100.0" layoutY="20.0" mnemonicParsing="false" onAction="#sendOffer" text="Send Offer" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
