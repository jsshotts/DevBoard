<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #102a50;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.PONavBar">
   <children>
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
      <Region HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER">
         <children>
            <HBox spacing="6.0">
               <children>
                  <Label text="Welcome" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="userName" text="Name" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label text="Project Owner" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Region layoutX="234.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <Button fx:id="createProjectButton" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="Create Project">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Region layoutX="95.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <Button fx:id="myProjectsButton" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="My Projects">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Region layoutX="220.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <Button fx:id="profileButton" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="Profile">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Region layoutX="557.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
