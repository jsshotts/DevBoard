<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.DevMyApplications">
   <children>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Project Offers" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <ScrollPane fx:id="projectOffers" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
              <content>
                <AnchorPane />
              </content>
               <VBox.margin>
                  <Insets right="5.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </ScrollPane>
         </children>
      </VBox>
      <VBox HBox.hgrow="ALWAYS">
         <children>
            <Label alignment="TOP_LEFT" contentDisplay="CENTER" text="Submitted Applications">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <ScrollPane fx:id="submittedApplications" fitToHeight="true" fitToWidth="true" VBox.vgrow="ALWAYS">
               <content>
                  <AnchorPane />
               </content>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </padding>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</HBox>
