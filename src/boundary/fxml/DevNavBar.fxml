<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #102a50;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.DevNavBar">
   <children>
      <Text fx:id="DevBoardLogo" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="DevBoard">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="44.0" />
         </font>
      </Text>
      <Region HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER">
         <children>
            <HBox spacing="6.0">
               <children>
                  <Label text="Welcome" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <Label fx:id="userName" text="Name" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Label alignment="TOP_LEFT" text="Developer" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Region layoutX="234.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER" prefHeight="64.0" prefWidth="195.0">
         <children>
            <Button fx:id="findProjectsButton" alignment="TOP_LEFT" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="Find Projects">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Region layoutX="95.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER" prefHeight="64.0" prefWidth="195.0">
         <children>
            <Button fx:id="activeProjectsButton" alignment="TOP_LEFT" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="Active Projects">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Region layoutX="220.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER" prefHeight="64.0" prefWidth="205.0">
         <children>
            <Button fx:id="myApplicationsButton" alignment="TOP_LEFT" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="My Applications">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Region layoutX="357.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
      <VBox alignment="CENTER" prefHeight="64.0" prefWidth="140.0">
         <children>
            <Button fx:id="profileButton" alignment="CENTER" minWidth="105.0" mnemonicParsing="false" onAction="#navBar" styleClass="navButton" text="Profile" textAlignment="CENTER">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Region layoutX="716.0" layoutY="20.0" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
